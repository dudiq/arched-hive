{
  "name": "coinote",
  "appName": "Coinote",
  "version": "5.0.1",
  "private": "true",
  "scripts": {
    "dev": "vite",
    "check-types": "tsc",
    "build:version": "node ./scripts/build-version.js",
    "build": "yarn check-types && yarn build:version && tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,gql,html,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@types/lz-string": "^1.3.34",
    "dexie": "^3.2.2",
    "jr-translate": "^2.0.1",
    "lz-string": "^1.4.4",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "preact": "^10.10.6",
    "preact-transitioning": "^1.3.2",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "wouter-preact": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@linaria/core": "^4.1.1",
    "@linaria/react": "^4.1.2",
    "@preact/preset-vite": "^2.3.0",
    "@swc/core": "^1.2.247",
    "@types/jest": "^29.0.0",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.0.1",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.23.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-max-params-no-constructor": "^0.0.4",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-inspect": "^0.6.1",
    "vite-plugin-linaria": "^1.0.0",
    "vite-plugin-pwa": "^0.12.6",
    "vite-plugin-swc-react": "^0.1.12"
  }
}
