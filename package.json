{
  "name": "coinote",
  "appName": "Coinote",
  "version": "5.0.1",
  "private": "true",
  "scripts": {
    "dev": "vite",
    "build:version": "node ./scripts/build-version.js",
    "build": "yarn build:version && tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,gql,html,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@types/lz-string": "^1.3.34",
    "fnscript": "^0.3.0",
    "jr-translate": "^2.0.1",
    "lz-string": "^1.4.4",
    "mobx": "^6.6.0",
    "mobx-react-lite": "^3.4.0",
    "preact": "^10.7.2",
    "preact-transitioning": "^1.3.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "wouter-preact": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@linaria/core": "^3.0.0-beta.19",
    "@linaria/react": "^3.0.0-beta.19",
    "@preact/preset-vite": "^2.2.0",
    "@swc/core": "^1.2.196",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-react": "^1.0.0",
    "eslint": "^8.17.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-max-params-no-constructor": "^0.0.4",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.52.2",
    "typescript": "^4.7.3",
    "vite": "^2.6.4",
    "vite-plugin-inspect": "^0.3.11",
    "vite-plugin-linaria": "^1.0.0",
    "vite-plugin-swc-react": "^0.1.12"
  }
}
