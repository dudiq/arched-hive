{
  "module":{
    "type":"es6"
  },
  "sourceMaps":true,
  "jsc":{
    "target":"ES2022",
    "keepClassNames":true,
    "parser":{
      "syntax":"typescript",
      "tsx":true,
      "dynamicImport":true,
      "decorators":true
      },
    "transform":{
      "legacyDecorator":true,
      "decoratorMetadata":true,
      "react":{
        "pragma":"h",
        "pragmaFrag":"Fragment",
        "runtime":"automatic",
        "throwIfNamespace":true
      }
    }
  },
  "minify":false
}
