{
  "name": "@repo/ui-kit",
  "version": "0.0.2",
  "private": true,
  "source": "src/index.ts",
  "types": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "sideEffects": false,
  "exports": {
    "./tailwind.base.css": "./dist/tailwind.base.css",
    "./default.tailwind.config": "./default.tailwind.config.cjs",
    ".": "./dist/index.mjs"
  },
  "scripts": {
    "types": "tsc",
    "copy-css": "node copy-css.mjs",
